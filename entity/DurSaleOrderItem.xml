<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    
    <entity entity-name="DurSaleOrderItem" package="durion.sales">
        <field name="orderItemId" type="id" is-pk="true"/>
        <field name="orderId" type="id"/>
        <field name="productId" type="id"/>
        <field name="productName" type="text-medium"/>
        <field name="quantity" type="number-decimal"/>
        <field name="unitPrice" type="currency-amount"/>
        <field name="lineAmount" type="currency-amount"/>
        <field name="discountPercent" type="number-decimal"/>
        <field name="taxAmount" type="currency-amount"/>
        <relationship type="one" related="durion.sales.DurSaleOrder" short-alias="order"/>
    </entity>

</entities>
