<?xml version="1.0" encoding="UTF-8"?>
<entities xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/entity-definition-3.xsd">
    
    <entity entity-name="DurSaleProduct" package="durion.sales">
        <field name="productId" type="id" is-pk="true"/>
        <field name="productName" type="text-medium"/>
        <field name="description" type="text-long"/>
        <field name="sku" type="text-short"/>
        <field name="categoryId" type="id"/>
        <field name="price" type="currency-amount"/>
        <field name="cost" type="currency-amount"/>
        <field name="quantityOnHand" type="number-decimal"/>
        <field name="isActive" type="text-indicator"/>
        <field name="imageUrl" type="text-medium"/>
    </entity>

</entities>
